{
  "name": "@nextcss/npm-package-template",
  "version": "1.0.0",
  "description": "Next.css modules starter template",
  "license": "MIT",
  "main": "dist/next.css",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "nextcss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Zsolt Tovis",
    "email": "tovis.zsolt@gmail.com",
    "url": "https://toviszsolt.github.io/"
  },
  "funding": "https://paypal.me/toviszsolt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextcss/npm-package-template.git"
  },
  "bugs": {
    "url": "https://github.com/nextcss/npm-package-template/issues"
  },
  "homepage": "https://github.com/nextcss",
  "scripts": {
    "build": "node-sass --output-style compact src/next.scss dist/next.css",
    "watch": "node-sass --watch --output-style compact src/next.scss dist/next.css"
  },
  "devDependencies": {
    "node-sass": "^5.0.0"
  }
}
